<?xml version="1.0" encoding="UTF-8" ?>
 <root>
     <$schema>https://developer.microsoft.com/json-schemas/teams/v1.17/MicrosoftTeams.schema.json</$schema>
     <id>408216d3-8b82-4c56-a595-3f48cd90ff06</id>
     <manifestVersion>1.17</manifestVersion>
     <version>1.0.0</version>
     <name>
         <short>Office UIB Web Embeddables</short>
         <full>Contoso Task Pane Add-in</full>
     </name>
     <description>
         <short>A template to get started.</short>
         <full>This is the template to get started.</full>
     </description>
     <developer>
         <name>Contoso</name>
         <websiteUrl>https://www.contoso.com</websiteUrl>
         <privacyUrl>https://www.contoso.com/privacy</privacyUrl>
         <termsOfUseUrl>https://www.contoso.com/servicesagreement</termsOfUseUrl>
     </developer>
     <icons>
         <outline>assets/outline.png</outline>
         <color>assets/color.png</color>
     </icons>
     <accentColor>#230201</accentColor>
     <localizationInfo>
         <defaultLanguageTag>en-us</defaultLanguageTag>
     </localizationInfo>
     <authorization>
         <permissions>
             <resourceSpecific>
                 <name>MailboxItem.Read.User</name>
                 <type>Delegated</type>
             </resourceSpecific>
         </permissions>
     </authorization>
     <validDomains>contoso.com</validDomains>
     <extensions>
         <requirements>
             <scopes>mail</scopes>
             <capabilities>
                 <name>Mailbox</name>
                 <minVersion>1.3</minVersion>
             </capabilities>
         </requirements>
         <runtimes>
             <requirements>
                 <capabilities>
                     <name>Mailbox</name>
                     <minVersion>1.3</minVersion>
                 </capabilities>
             </requirements>
             <id>TaskPaneRuntime</id>
             <type>general</type>
             <code>
                 <page>https://web-embeddables.vercel.app/dist/taskpane.html</page>
             </code>
             <lifetime>short</lifetime>
             <actions>
                 <id>TaskPaneRuntimeShow</id>
                 <type>openPage</type>
                 <pinnable>false</pinnable>
                 <view>dashboard</view>
             </actions>
         </runtimes>
         <runtimes>
             <id>CommandsRuntime</id>
             <type>general</type>
             <code>
                 <page>https://web-embeddables.vercel.app/dist/commands.html</page>
                 <script>https://web-embeddables.vercel.app/dist/commands.js</script>
             </code>
             <lifetime>short</lifetime>
             <actions>
                 <id>action</id>
                 <type>executeFunction</type>
             </actions>
         </runtimes>
         <ribbons>
             <contexts>mailRead</contexts>
             <tabs>
                 <builtInTabId>TabDefault</builtInTabId>
                 <groups>
                     <id>msgReadGroup</id>
                     <label>Contoso Add-in</label>
                     <icons>
                         <size>16</size>
                         <url>https://web-embeddables.vercel.app/dist/assets/icon-16.png</url>
                     </icons>
                     <icons>
                         <size>32</size>
                         <url>https://web-embeddables.vercel.app/dist/assets/icon-32.png</url>
                     </icons>
                     <icons>
                         <size>80</size>
                         <url>https://web-embeddables.vercel.app/dist/assets/icon-80.png</url>
                     </icons>
                     <controls>
                         <id>msgReadOpenPaneButton</id>
                         <type>button</type>
                         <label>Show Task Pane</label>
                         <icons>
                             <size>16</size>
                             <url>https://web-embeddables.vercel.app/dist/assets/icon-16.png</url>
                         </icons>
                         <icons>
                             <size>32</size>
                             <url>https://web-embeddables.vercel.app/dist/assets/icon-32.png</url>
                         </icons>
                         <icons>
                             <size>80</size>
                             <url>https://web-embeddables.vercel.app/dist/assets/icon-80.png</url>
                         </icons>
                         <supertip>
                             <title>Show Task Pane</title>
                             <description>Opens a pane displaying all available properties.</description>
                         </supertip>
                         <actionId>TaskPaneRuntimeShow</actionId>
                     </controls>
                     <controls>
                         <id>ActionButton</id>
                         <type>button</type>
                         <label>Perform an action</label>
                         <icons>
                             <size>16</size>
                             <url>https://web-embeddables.vercel.app/dist/assets/icon-16.png</url>
                         </icons>
                         <icons>
                             <size>32</size>
                             <url>https://web-embeddables.vercel.app/dist/assets/icon-32.png</url>
                         </icons>
                         <icons>
                             <size>80</size>
                             <url>https://web-embeddables.vercel.app/dist/assets/icon-80.png</url>
                         </icons>
                         <supertip>
                             <title>Perform an action</title>
                             <description>Perform an action when clicked.</description>
                         </supertip>
                         <actionId>action</actionId>
                     </controls>
                 </groups>
             </tabs>
         </ribbons>
     </extensions>
 </root>
